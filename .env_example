# 应用配置
APP_NAME=tradewinds
APP_ENV=development
APP_PORT=3000
APP_HOST=127.0.0.1

# 数据库配置
DATABASE_URL=mysql://root:123456@localhost:3306/tradewinds
DATABASE_POOL_SIZE=10

# Redis配置
REDIS_URL=redis://localhost:6379
REDIS_POOL_SIZE=10

# JWT配置
JWT_SECRET=your_jwt_secret_key_change_this_in_production
JWT_EXPIRATION=1440  # 24小时（分钟）

# RabbitMQ配置
RABBITMQ_URL=amqp://guest:guest@localhost:5672

# 日志配置
LOG_LEVEL=debug  # 调试时使用 debug，生产环境使用 info
LOG_FILE=logs/app.log
RUST_LOG=debug,sqlx=info,sea_orm=debug,tradewinds=debug
RUST_BACKTRACE=1  # 显示完整的错误堆栈

# Server配置
SERVER_HOST=127.0.0.1
SERVER_PORT=8080 